{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "src/main.ts": {
      "runtime": "@godsreveal/vercel-bun@0.0.50"
    }
  },
  // Optional: use if you want all /api routes to be handled by /api/index.ts
  "rewrites": [{ "source": "/(.*)", "destination": "/src/main.ts" }]
}
